root = true

[*]
charset = utf-8
end_of_line = crlf
indent_size = 2
indent_style = space
insert_final_newline = false
max_line_length = 120
tab_width = 2
trim_trailing_whitespace = false

[*.rs]
max_line_length = 100
indent_size = 2

[{*.ats,*.cts,*.mts,*.ts}]
indent_size = 2

[*.json]
indent_size = 2